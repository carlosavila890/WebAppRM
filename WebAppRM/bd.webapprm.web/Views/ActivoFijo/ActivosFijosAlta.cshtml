@model List<RecepcionActivoFijoDetalleSeleccionado>
<div id="content">
    <p>
        <a asp-action="GestionarAlta" class="btn btn-success">Nueva Alta</a>
    </p>
    <section id="widget-grid" class="">
        <div class="row">
            <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="jarviswidget jarviswidget-color-darken" id="wid-id-0" data-widget-editbutton="false">
                    <header>
                        <span class="widget-icon"> <i class="fa fa-table"></i> </span>
                        <h2>Listado de Altas de Activos Fijos</h2>
                    </header>
                    <div>
                        <div class="jarviswidget-editbox">
                        </div>
                        <div class="widget-body no-padding">
                            @{await Html.RenderPartialAsync("_ListadoDetallesActivosFijos", Model);}
                        </div>
                    </div>
                </div>
            </article>
        </div>
    </section>
</div>
@section Scripts {
    <script src='~/js/activoFijo/listadoSeleccion.min.js'></script>
    <script>
        jQuery(document).ready(function () {
            initDataTableFiltrado("tableDetallesActivoFijo", []);
        });
    </script>
}